{
  "hosting": [
    {
      "public": "alcohol/build",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "alcohol/index.html"
        }
      ],
      "target": "alcohol"
    },
    {
      "public": "ostle/build",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ],
      "target": "ostle"
    }
  ]
}
